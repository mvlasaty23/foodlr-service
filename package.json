{
  "name": "foodlr-service",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "author": "Michael Vlasaty",
  "license": "MIT",
  "main": "serverless.ts",
  "scripts": {
    "postinstall": "husky install",
    "dynamodb:install": "sls dynamodb install",
    "dynamodb:start": "sls dynamodb start --migrate",
    "sls:deploy:dev": "sls deploy --stage dev",
    "sls:deploy:prod": "sls deploy --stage prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "format": "prettier -c .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint"
  },
  "lint-staged": {
    "*.{ts}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.json,md": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@middy/core": "^2.5.4",
    "@middy/http-json-body-parser": "^2.5.4",
    "aws-sdk": "^2.1053.0",
    "rxjs": "^7.5.1",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@serverless/typescript": "^2.70.0",
    "@types/aws-lambda": "^8.10.89",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-jest": "^27.4.6",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "json-schema-to-ts": "^1.6.4",
    "lint-staged": "^12.1.7",
    "npm-check-updates": "^12.1.0",
    "prettier": "^2.5.1",
    "serverless": "^2.70.0",
    "serverless-bundle": "^5.2.0",
    "serverless-dotenv-plugin": "^3.12.2",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-offline": "^8.3.1",
    "serverless-webpack": "^5.6.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-node-externals": "^3.0.0"
  }
}
